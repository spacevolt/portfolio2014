{
  "files": [
    {
      "from": "controller.coffee.hbs",
      "to": "app/module/{{name}}/{{name}}-controller.coffee"
    }
  ],
  "dependencies": [
    {"type": "style", "name": "{{name}}", "parentPath": "{{parentPath}}/styles"},
    {"type": "template", "name": "{{name}}", "parentPath": "{{parentPath}}/templates"}
  ]
}
